{
    "manifest_version":2,

    "name":"Crude XSS Blocker",
    "version":"0.6.0",

    "content_scripts":[{
        "matches":["<all_urls>"],
        "js":["contentScript.js"],
        "run_at":"document_start"
    }],

    "background":{
        "scripts":["background.js"]
    },

    "permissions":[
        "<all_urls>",
        "notifications"
    ],

    "icons":{
        "48":"cxb.svg",
        "120":"cxb.svg"
    },

    "web_accessible_resources": [
        "cxb.png"
    ]
}