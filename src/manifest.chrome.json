{
    "manifest_version":3,

    "name":"Crude XSS Blocker",
    "version":"0.6.0",

    "content_scripts":[{
        "matches":["<all_urls>"],
        "js":["contentScript.js"],
        "run_at":"document_start"
    }],

    "background":{
        "scripts":["background.js"]
    },

    "permissions":[
        "<all_urls>",
        "notifications"
    ]
}